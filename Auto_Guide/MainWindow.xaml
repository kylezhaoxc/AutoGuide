<Window x:Class="Auto_Guide.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        Title="MainWindow" MouseDoubleClick="Window_MouseDoubleClick">
    <Grid>
        <Ellipse Height="20" Fill="Red" VerticalAlignment="Top" HorizontalAlignment="Left" Margin="10,5,0,0" Name="ell_flag"
		         Stroke="Red" Width="20" />
        <Image x:Name="cam" HorizontalAlignment="Left" Height="300" Margin="10,30,0,0" VerticalAlignment="Top" Width="400" />
        <Image x:Name="cam_right" HorizontalAlignment="Left" Height="300" Margin="415,30,-55,0" VerticalAlignment="Top"
		       Width="400" />
        <Label x:Name="status" Content="Label" HorizontalAlignment="Left" Margin="22,355,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.701,0.577"/>
        <Label x:Name="node" Content="Label" HorizontalAlignment="Left" Margin="22,428,0,0" VerticalAlignment="Top" RenderTransformOrigin="1.701,0.577"/>

    </Grid>
</Window>
